@model IEnumerable<WebApp.Models.Message>
<div class="main-wrapper">
	<div class="section-service">
		<div class="padding-global">
			<div class="container-large">
				<div class="padding-section-large">
					<div class="service-component">
						<h2 class="heading-style-h2">All messages</h2>
						<div class="messages" style="display: flex;
							flex-direction: column;
							gap: 20px;
							padding: 20px;
							background-color: #f4f4f4;
							border-radius: 8px;
							max-width: 800px;
							margin: 0 auto;
">			
							@foreach (var msg in Model)
							{
								<div class="message" style="padding: 20px;
							background-color: #ffffff;
							border: 1px solid #e0e0e0;
							border-radius: 8px;
							box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1
							.message p {
							margin: 0 0 10px 0;
							}

							.message p:last-child {
							margin-bottom: 0;
							}

							.message-fullname {
							font-weight: bold;
							font-size: 1.2rem;
							}

							.message-email {
							color: #555555;
							font-size: 1rem;
							}

							.message-text {
							font-size: 1rem;
							line-height: 1.5;
							}">
									
									<p>Fullname: @msg.Fullname</p>
									<p>Email: @msg.Email</p>
									<p>Message: @msg.MessageText</p>
									<p>Company: @msg.Company</p>
									<p>Budget : @msg.Budget</p>
									<form method="post" asp-controller="About" asp-action="Delete">
										<input type="hidden" name="id" value="@msg.Id" />
										<button type="submit">Удалить</button>
									</form>
								</div>
							}

						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
